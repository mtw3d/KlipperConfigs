#! /bin/bash
cd ~/klipper_config/

cp -n mtw_klipper_config/sample_configs/printer.cfg printer.cfg
cp -n mtw_klipper_config/sample_configs/macros-custom.cfg macros-custom.cfg
cp mtw_klipper_config/update_mtw_configs ~/update_mtw_configs
mkdir ~/config_backups 2>/dev/null
